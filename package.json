{
  "name": "is-honcode-certified",
  "version": "0.0.4",
  "description": "Check if a website is HONcode certified",
  "keywords": [
    "HONcode",
    "Certification",
    "HON",
    "Health On the Net"
  ],
  "author": "William Belle <william.belle@gmail.com>",
  "license": "Apache-2.0",
  "repository": "https://github.com/healthonnet/is-honcode-certified.git",
  "main": "src/lib/index.js",
  "bin": "src/bin/cli.js",
  "files": [
    "src/lib/index.js",
    "src/bin/cli.js"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "jshint . && eslint . && mocha",
    "coverage": "istanbul cover _mocha",
    "coveralls": "npm run coverage -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "jshint": "~2.9.5",
    "eslint": "^5.4.0",
    "mocha": "^4.0.1"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "it": true,
      "describe": true
    },
    "rules": {
      "no-console": "off"
    }
  },
  "eslintIgnore": [
    "node_modules/**",
    "coverage/**"
  ],
  "dependencies": {
    "honcode-certification-list": "0.0.6",
    "honcode-certification-utils": "0.0.2",
    "yargs": "^10.0.3"
  }
}
